<% @title="Log In" %>
<p><%= flash[:error] %>

<%= form_for :user, url:{action:'login'} do |f| %>
<div class="field">
<%= f.label :email  %>
<%= f.text_field :email  %>	
</div>

<div class="field">
<%= f.label :password  %>
<%= f.password_field :password %>
</div>

<p><%= f.submit 'Log In', class:'btn btn-primary'%>
<% end %>
